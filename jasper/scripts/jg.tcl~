
clear -all

global env


set design_top        $env(DUT_TOP)
set lab_path          $env(FPV_PATH)

analyze -sv09 ${lab_path}/rtl/square.v \

analyze -sv09 $lab_path/bind/square_bind.sv \
			  $lab_path/sva/square_sva.sv

elaborate  -top square -disable_auto_bbox -create_related_covers {precondition witness}

clock clk
reset ~resetn

get_design_info

set_engine_mode auto

set_proofgrid_manager on

##prove -bg -all
